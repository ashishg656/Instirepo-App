package com.instirepo.app.fragments;

import android.os.Bundle;
import android.support.v4.app.DialogFragment;
import android.view.LayoutInflater;
import android.view.View;
import android.view.ViewGroup;

import com.instirepo.app.R;

public class SeenByPeopleFragment extends DialogFragment {

	public static SeenByPeopleFragment newInstance(Bundle b) {
		SeenByPeopleFragment frg = new SeenByPeopleFragment();
		frg.setArguments(b);
		return frg;
	}

	public SeenByPeopleFragment() {
		int style = DialogFragment.STYLE_NORMAL;
		int theme = R.style.BottomSheet_Dialog;
		setStyle(style, theme);
	}

	@Override
	public View onCreateView(LayoutInflater inflater, ViewGroup container,
			Bundle savedInstanceState) {
		View v = inflater.inflate(
				R.layout.seen_by_poeple_dialog_fragment_layout, container,
				false);

		return v;
	}

	@Override
	public void onActivityCreated(Bundle arg0) {
		super.onActivityCreated(arg0);
	}

}
